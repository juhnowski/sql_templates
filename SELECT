SELECT LS.*
FROM ( SELECT SNO , SNAME , STATUS
FROM S
WHERE CITY = 'London' ) AS LS

SELECT S.SNO , S.SNAME , S.STATUS , S.CITY
FROM S
WHERE NOT EXISTS
( SELECT *
FROM SP
WHERE SP.PNO = S.SNO )

SELECT ...
FROM ...
WHERE CNO = SNO IGNORE DOMAIN CHECKS

SELECT SNO
FROM S
WHERE ( STATUS , CITY ) = ( 20 , 'London' )

SELECT SNO
FROM S
WHERE STATUS = 20 AND CITY = 'London'

SELECT SNO
FROM S
WHERE ( STATUS , CITY ) <> ( 20 , 'London' )

SELECT SNO
FROM S
WHERE STATUS <> 20 OR CITY <> 'London'

SELECT SNO
FROM S
WHERE ( STATUS , CITY ) > ( 20 , 'London' )

SELECT SNO
FROM S
WHERE STATUS > 20 OR ( STATUS = 20 AND CITY > 'London' )

SELECT SNO
FROM S
WHERE ( STATUS ) = ( 20 )

SELECT SNO
FROM S
WHERE STATUS = 20

SELECT P.PNO
FROM P
WHERE P.PNAME = 'Screw'
OR P.PNO IN
( SELECT SP.PNO
FROM SP
WHERE SP.SNO = 'S1' )						P1 * 3, P2 * 1

SELECT SP.PNO
FROM SP
WHERE SP.SNO = 'S1'
OR SP.PNO IN
( SELECT P.PNO
FROM P
WHERE P.PNAME = 'Screw' )					P1 * 2, P2 * 1

SELECT P.PNO
FROM P , SP
WHERE ( SP.SNO = 'S1' AND
SP.PNO = P.PNO )
OR P.PNAME = 'Screw'						P1 * 5, P2 * 3

SELECT SP.PNO
FROM P , SP
WHERE ( SP.SNO = 'S1' AND
SP.PNO = P.PNO )
OR P.PNAME = 'Screw'						P1 * 6, P2 * 2

SELECT P.PNO
FROM P
WHERE P.PNAME = 'Screw'
UNION ALL
SELECT SP.PNO
FROM SP
WHERE SP.SNO = 'S1'							P1 * 2, P2 * 2

SELECT DISTINCT P.PNO
FROM P
WHERE P.PNAME = 'Screw'
UNION ALL
SELECT SP.PNO
FROM SP
WHERE SP.SNO = 'S1'							P1 * 2, P2 * 2

SELECT P.PNO
FROM P
WHERE P.PNAME = 'Screw'
UNION ALL
SELECT DISTINCT SP.PNO
FROM SP
WHERE SP.SNO = 'S1'							P1 * 2, P2 * 2

SELECT DISTINCT P.PNO
FROM P
WHERE P.PNAME = 'Screw'
OR P.PNO IN
( SELECT SP.PNO
FROM SP
WHERE SP.SNO = 'S1' )						P1 * 1, P2 * 1

SELECT DISTINCT SP.PNO
FROM SP
WHERE SP.SNO = 'S1'
OR SP.PNO IN
( SELECT P.PNO
FROM P
WHERE P.PNAME = 'Screw' )					P1 * 1, P2 * 1

SELECT P.PNO
FROM P
GROUP BY P.PNO , P.PNAME
HAVING P.PNAME = 'Screw'
OR P.PNO IN
( SELECT SP.PNO
FROM SP
WHERE SP.SNO = 'S1' )						P1 * 3, P2 * 1

SELECT P.PNO
FROM P , SP
GROUP BY P.PNO , P.PNAME , SP.SNO , SP.PNO
HAVING ( SP.SNO = 'S1' AND
SP.PNO = P.PNO )
OR P.PNAME = 'Screw'						P1 * 5, P2 * 3

SELECT P.PNO
FROM P
WHERE P.PNAME = 'Screw'
UNION
SELECT SP.PNO
FROM SP
WHERE SP.SNO = 'S1'							P1 * 1, P2 * 1

